pipeline {

  agent {
    docker { image 'node:latest' }
  }

  stages {

    stage('Install') {
      steps { sh 'cd Poker-Frontend && sudo npm install' }
    }

      stage('e2e tests') {
            steps { sh 'cd Poker-Frontend && ng e2e' }
        }


    stage('Build') {
      steps { sh 'cd Poker-Frontend && ng build' }
    }
  }
}
